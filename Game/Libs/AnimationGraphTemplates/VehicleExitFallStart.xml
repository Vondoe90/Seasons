<AGTemplate name="VehicleExitFallStart" extend="VehicleTransition">
	<Param name="animation" type="string"/>
	<Param name="playback_speed" type="float"/>

	<MovementControlMethod horizontal="Animation" vertical="Animation"/>

	<TransitionParamsLayer1
		Structure=""
		TransitionTime="0.0"
		KeyTime="-1"
		LoopAnimation="0"
		RepeatLastKey="1"
		VTimeWarping="0"
		WaitForAnimation="1"
		/>

	<AnimationLayer1
		animation1="$animation"
		ensureInStack="0"
		stickyOutTime="-1"
		stayInStateUntil="0"
		forceLeaveWhenFinished="0"
		speedMultiplier="$playback_speed"
		/>

	<!--MusicDelayLeaving time="1.2" speedupthreshold="0"/-->

</AGTemplate>
