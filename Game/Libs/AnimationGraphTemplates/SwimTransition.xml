<AGTemplate name="SwimTransition">
	<Param name="animation" type="string"/>
	<Param name="playback_speed" type="float"/>
	<Param name="structure" type="string"/>
  <Param name="transition_time" type="float"/>

	<MovementControlMethod horizontal="Entity" vertical="Animation"/>
	<ColliderMode mode="Pushable"/>

	<TransitionParamsLayer1
		Structure="$structure"
		TransitionTime="0.2"
		KeyTime="-1"
		LoopAnimation="0"
		RepeatLastKey="1"
		VTimeWarping="0"
		WaitForAnimation="1"
		/>

	<AnimationLayer1
		animation1="$animation"
		aimPose=""
		aimPose2=""
		ensureInStack="0"
		stickyOutTime="-1"
		forceLeaveWhenFinished="0"
		speedMultiplier="$playback_speed"
		/>
</AGTemplate>

